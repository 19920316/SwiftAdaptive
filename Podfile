
source 'https://github.com/CocoaPods/Specs.git'
# 说明引入的框架支持的平台是ios，版本是9.0
platform :ios, '9.0'

# 忽略引入库的所有警告（强迫症者的福音啊）
inhibit_all_warnings!

#swift 项目中通过 pod引入第三方必须加上 use_frameworks！
use_frameworks!

target 'SwiftTest' do
    pod 'SnapKit'
    pod 'Kingfisher','~> 4.7.0'
    pod 'ReactiveSwift', '~> 4.0.0'
end

